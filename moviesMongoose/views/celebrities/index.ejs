<h2> My Celebrities </h2>

<ul> 
    <% celebrities.forEach((celebs) => { %>
        <li>
            <h3> <a href="/celebrities/<%= celebs._id %>"> <%= celebs.name %> </a> </h3>
            <form action="/celebrities/<%= celebs._id%>/delete" method="POST">
                <button type="submit"> Delete </button>
            </form>
            <a href="/celebrities/<%= celebs._id %>/edit"> edit </a>
        </li>
   <% }) %>
</ul>

<a href="/"> Return to home page </a>
