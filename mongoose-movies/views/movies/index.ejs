<h2>Movies</h2>
<ul>
  <% movies.forEach(movie => { %>
    <li><%= movie.title %></li>
    <a href="/movies/<%= movie._id %>">See movie</a>
    <form action="/movies/<%= movie._id %>/delete" method="POST">
      <button type="submit">Delete movie</button>
    </form>
  <% }); %>
</ul>
<a href="/movies/new">Create new movie</a>
