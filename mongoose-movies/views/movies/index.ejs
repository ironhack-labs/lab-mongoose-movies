<h2>Movies</h2>

<ul>
  <% movies.forEach((movie) => { %>
    <li>
      <a href="movies/<%= movie._id %>"><%= movie.title %></a>
      <a href="movies/<%= movie._id %>/edit">Edit</a>
      <form action="movies/<%= movie._id %>/delete" method="post"><button type="submit">Delete</button></form>
    </li>
  <% }) %>
</ul>

<a href="/movies/new">Add a new movie</a>
