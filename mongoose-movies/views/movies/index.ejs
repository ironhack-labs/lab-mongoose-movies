<h2>Movies</h2>
<ul>
  <% movies.forEach((movie) => { %>
    <li><a href="/movies/<%= movie._id%>"><%= movie.title%></a>
    <a href="/movies/<%= movie._id%>/edit">edit</a>
    <form action="/movies/<%= movie._id%>/delete" method="post">
      <button type="submit" name="button">delete</button>
    </form>
    </li>
  <% }) %>
</ul>
<a href="/movies/new">Introduce new Movie! (please...)</a>
