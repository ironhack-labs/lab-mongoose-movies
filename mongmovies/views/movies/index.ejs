<h2>Movies List</h2>
<% moovie.forEach((movie)=>{%>
  <li><a href="/movies/<%=movie._id%>"><%= movie.title %></a></li>
  <form action="/movies/<%=movie._id%>/edit" method="POST">
    <button type="submit">Edit</button>
  </form>
  <form action="/movies/<%=movie._id%>/delete" method="POST">
    <button type="submit">Delete</button>
  </form>
<%})%>

<a href="/movies/new"><button type="Submit">Add a movie</button></a>
