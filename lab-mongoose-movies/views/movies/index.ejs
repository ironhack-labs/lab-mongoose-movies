<h2> Movies </h2>

<ul>
  <% movies.forEach((movie) => { %>
    <li>
      <a href="/movies/<%=movie._id%>"><%= movie.title %>
      <form action="/movies/<%=movie._id%>/delete" method="POST">
        <button type="submit">
          Delete
        </button>
      </form>
      </a>
    </li>
  <% }); %>
</ul>
