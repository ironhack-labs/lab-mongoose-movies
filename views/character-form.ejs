<h2> Add a New DBZ Character </h2>

<!-- -->
<form method="post" action="/characters">
  <label for="name">Name</label>
  <input id="name" type="text" name="name"><br>

  <label for="species">Species</label>
  <input id="species"type="text" name="species"><br>

  <label for="imgUrl">Image URL</label>
  <input id="imgUrl"type="text" name="imgUrl"><br>

  <label for="powerLevel">Power Level</label>
  <input id="powerLevel"type="text" name="powerLevel"><br>

  <label for="catchPhrase">Catch Phrase</label>
  <textarea id="catchPhrase" type="text" name="catchPhrase"></textarea><br>
  <button>Submit</button>
</form>
