<h1>PELICULAS</h1>
{{#each movies}}
    <p>
      <a href="movie/{{this._id}}">{{this.title}}</a>
      <a href="/movies/edit?movie_id={{this._id}}" class="edit-button">EDIT</a>
      <a href="/movies/delete?movie_id={{this._id}}" class="delete-button">DELETE</a>
    </p>
{{/each}}
<br>
<a href="/">Return</a>
