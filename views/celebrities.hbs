<h1>CELEBRIDADES</h1>
{{#each celebrities}}
  <p>
    <a href="celebrity/{{this._id}}">{{this.name}}</a>
    <a href="/celebrities/edit?celebrity_id={{this._id}}" class="edit-button">EDIT</a>
    <a href="/celebrities/delete?celebrity_id={{this._id}}" class="delete-button">DELETE</a>
  </p>
{{/each}}
<br>
<a href="/">Return</a>
