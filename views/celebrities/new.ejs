<h2>Create a new Celebrity</h2>

<form method="post" action="/celebrities">
  <div>
    <label for="_name"> Name: </label>
    <input type="text" name="_name">
  </div>

  <div>
    <label for="_occupation"> Occupation: </label>
    <input type="text" name="_occupation">
  </div>

  <div>
    <label for="_catchPhrase"> Catch Phrase: </label>
    <input type="text" name="_catchPhrase">
  </div>

  <input type="submit" name="" value="ADD">
</form>
