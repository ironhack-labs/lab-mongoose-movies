<h2>Celebrities Webpage</h2>

<ul>
  <% celebrities.forEach( aCeleb => { %>
    <p><%= aCeleb.name %></p>
    <a href="/celebrities/<%= aCeleb._id %>"> View details</a>
    <a href="/celebrities/<%= aCeleb._id %>/delete"> Delete</a>
  <% }) %>
</ul>

<a href="celebrities/new">Add a new Celebrity</a>
