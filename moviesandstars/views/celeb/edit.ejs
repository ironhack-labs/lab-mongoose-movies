<form action="/celeb/<%= celeb._id %>" method="POST">
  <label for="name">Name</label>
  <input type="text" id="name" name="celebName" value="<%=celeb.celebName%>">

  <label for="name">Occupation</label>
  <input type="text" id="occupation" name="celebOccup" value="<%=celeb.celebOccup %>">

  <label for="name">Phrase</label>
  <input type="text" id="phrases" name="celebPhrase" value="<%=celeb.celebPhrase %>">

  <button type="submit">Update Celeb</button>
</form>
