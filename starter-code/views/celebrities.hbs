<h2>Celebrities</h2>

{{!-- {{#if msg}}
  <div class = "msg">{{msg}}</div>
{{/if}} --}}
{{#each celebrities}}
    <p>
        <a href="celebrities/{{this._id}}">{{this.name}}</a>
         <a href="/celebrities/edit?celebrityId={{this._id}}" class="edit-button">EDIT</a>
        <a href="/celebrities/del/{{this._id}}" class="del-button">DELETE</a>        
    </p>
{{/each}}