<h1>List of celebrities</h1>				
<ul>				
	{{#each celebrities}}			
		<li>		
			<a href="/celebrities/{{this._id}}">	
				{{this.name}}
			</a>
			(
				<a href="/celebrities/{{this._id}}/edit">Edit</a>
				<a href="/celebrities/{{this._id}}/delete">Delete</a>
			)
		</li>		
	{{/each}}			
</ul>				

<a href="/celebrities/new">Add a celebrity</a>