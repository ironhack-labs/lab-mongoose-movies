<div class="buttonsSection">
  <a id="backButton" href="/celebrities"><i class="fas fa-arrow-left"></i></a>
</div>

<section class="celebritySection">
  <div class="resume">
    {{#each celebrity}}
    <h1>{{this.name}}</h1>
    <img src="{{this.img}}" alt="">
    <h2>{{this.occupation}}</h2>
    <p>{{this.catchPhrase}}</p>
    {{/each}}
  </div>
</section>