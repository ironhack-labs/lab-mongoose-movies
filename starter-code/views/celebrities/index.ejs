<h2> CELEBRITIES</h2>
<ul>
    <% celebrities.forEach((celebr) => { %>
        <li>
            <%= celebr.name %>
            <a href="/show/<%= celebr._id %>">View Details</a>
            <a href="/show/<%= celebr._id %>/edit">Edit Celebrity</a>
        </li>
        <%  }) %>
        <a href="/">GO HOME</a>
</ul>

<a href="/new">Create a new celebrity</a>
