<!DOCTYPE html>
<html>
  <head>
    <title> Celebrities </title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h2>Celebrities</h2>
    <ul>
      <% celebrities.forEach((celebrity) => { %>
        <li><a href="celebrities/<%=celebrity._id%>"><%= celebrity.name%></a></li>
      <% })%>
    </ul>

    <a href="/celebrities/new">Create Celebrity</a>
  </body>
</html>
