<h2>Celebrity list</h2>
<ul>
    {{#each theirList }}
    <li>
        <a href="celebrity/{{this._id}}">{{this.name}}</a>
        <button><a href ="edit/celebrity/{{this._id}}">Edit</a></button>
        <button><a href="delete/celebrity/{{this._id}}">Delete</a></button>
    </li>
    {{/each}}
</ul>

<a href="/celebrities/new">Add new celebrity</a>