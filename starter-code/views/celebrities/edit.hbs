<h2>Edit celebrity</h2>
{{#with seed}}
<form method="POST" action="/celebrities/edit/{{this._id}}">
  <input class="form-control form-control-lg" type="text" value="{{this.name}}" name="name">
  <input class="form-control" type="text" value="{{this.occupation}}" name="occupation">
  <input class="form-control form-control-sm" type="text" value="{{this.catchPhrase}}" name="catchPhrase">
  <button type="submit" class="btn btn-success button2">Update</button>
</form>
{{/with}}