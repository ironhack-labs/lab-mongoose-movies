<h1 class="display-4 m-5">Celebrities</h1>
<div class="container m-5">
  <div class="row">
    <div class="col">

      <div class="card m-3">
        <div class="card-header">
          {{!-- llamada a celebrity.js y a sus propiedades (esquema) --}}
          {{ celebrity.name}}
        </div>
        <div class="card-body">
          <h5 class="card-title">{{ celebrity.catchPhrase }}</h5>
          <p class="card-text">{{ celebrity.occupation }}</p>
        </div>
      </div>
    </div>
  </div>
</div>