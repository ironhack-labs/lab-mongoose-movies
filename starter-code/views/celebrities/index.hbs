<h3><a href="/">Go Back!</a></h3>
<h3><a href="celebrities/new">Create a New Celebrity</a></h3>
{{#each allCelebs as |celeb|}}
<h2>{{celeb.name}}</h2>
<p><a href="/celebrities/{{celeb.id}}">See More</a>
<form action="/celebrities/{{celeb.id}}/delete" method="POST">
  <button type="submit">Delete</button>
</form>
<form action="/celebrities/{{celeb.id}}/edit">
  <button type="submit">Edit</button>
</form>
{{/each}}