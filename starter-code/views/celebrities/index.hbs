<h2>Celebrity List</h2>
<a href="/celebrities/new"><button>Add New Celebrity</button></a>
{{#each celebs}}
  <a href="../celebrities/{{_id}}"><p>{{this.name}}</p></a>
 <a href="/celebrities/{{_id}}/edit"><button>Edit</button></a>
  <form action="/celebrities/{{_id}}/delete" method="POST">
    <button type="submit">Delete</button>
  </form>
{{/each}}