<h2>Celebrities</h2>

{{#each celebrities}}
<div>
  name: {{name}}
  
  {{!ocupation: {{occupation}}

  {{!catchPhrase :{{catchPhrase}} 

  <a href="/celebrities/{{_id}}">Details</a>
  <a href="/celebrities/new">Create a new celebrity</a>

  <form action="/celebrities/{{id}}/delete" method="post">
  <button type="submit"> Remove</button>
  </form>
</div>

{{/each}}