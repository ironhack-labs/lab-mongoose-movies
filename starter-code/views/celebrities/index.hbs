<h2>Celebrities</h2>
<a href="/celebrities/new">Add new celebrity</a>
<br>
{{#each allCelebritiesFromDb}}
  <span><a href="/celebrities/{{this._id}}">{{this.name}}</a></span>
  <form action="/celebrities/{{this._id}}/delete" method="post">
    <button type='submit'>DELETE</button>
  </form>
  <form action="/celebrities/{{this.id}}/edit" method="GET">
    <button type='submit'>EDIT</button>
  </form>
{{/each}}