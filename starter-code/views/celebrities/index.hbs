<h2>Celebrities</h2>
  {{#each celebs}}
  <ul>
    <li>{{this.name}}</li>
    <li>{{this.occupation}}</li>
    <li>{{this.catchPhrase}}</li>
    <a href="/celebrities/{{this._id}}">Show more</a>
  </ul>
    <form action="/celebrities/{{this._id}}/delete" method="post">
      <button type="submit">Delete</button>
    </form>
{{/each}}

<h4><a href="/celebrities/new">Add new celebritie</a></h4>
