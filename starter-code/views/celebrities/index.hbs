<h2>Celebrities Page</h2>
{{#each celebrity}}
  <h3><a href="/celebrities/{{id}}">{{name}}</a></h3>
  <form action="/celebrities/{{id}}/delete" method="POST">
    <button type="submit">Delete</button>
  </form>
  <form action="/celebrities/{{id}}/edit" method="GET">
    <button type="submit">Edit</button>
  </form>
  <hr>

{{/each}}