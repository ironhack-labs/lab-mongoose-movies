<h2>List of Celebrities</h2>
<a href="celebrities/new">Add celebrity</a>

{{#each celebritiesFromDB}}
<p>{{this.name}}</p>
<a href="/celebrities/{{this.id}}">See details</a>
<form action="/celebrities/{{this.id}}/delete" method="POST">
        <button type="submit">Delete</button>
        <a href="/celebrities/{{this.id}}/edit">Edit</a>
</form>
{{/each}}