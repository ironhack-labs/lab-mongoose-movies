<div id="back-col">
  <div id="marg">
    <h1>MOVIES</h1>
    <a class="btn" href="/movies/newMov">New movie</a>
    <ul>
      {{#each movies as |movie|}}
    <div class="row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-content">
          <a href="/movies/{{movie._id}}">
          <span class="card-title">{{movie.title}}</span></a>
        </div>
        <div class="card-action">
          <a href="/movies/{{movie._id}}/edit">Editar</a>
          <a href="/movies/{{movie._id}}/delete">Eliminar</a>
        </div>
      </div>
    </div>
  </div>
      {{/each}}
    </ul>
  </div>
</div>