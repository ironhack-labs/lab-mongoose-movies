<ul>
  <li>
    <h2>{{{movie.title}}}</h2>
    <p>Genre: {{{movie.genre}}}</p>
    <p>Plot: {{movie.plot}} </p>
    <p>Stars:</p>
    <ul class="detail-page">
      {{#each celeberty}}
      <li class="detail-page">
        <p>{{this._celebertyId.name}}</p>
      </li>
      {{/each}}

    </ul>
  </li>

</ul>

<a class="button" href="/movies/">Go back</a>
<a class="button" href="/movies/{{movie._id}}/delete">Delete</a>
<a class="button" href="/movies/{{movie._id}}/edit-movie">Edit</a>
<a class="button" href="/movies/{{movie._id}}/add-actor">Add Actor</a>