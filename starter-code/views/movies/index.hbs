<h1>Movies</h1>
<a href="/movies/add-new-movie" class="create">Add a movie</a>
<ul id="movie-list">
    {{#each movies}}
    <li class="item">
        <p class="title">{{this.title}}</p>
        <p class="genre">{{this.genre}}</p>
        <p class="plot">{{this.plot}}</p>
        <a href="/movies/{{this._id}}/">See more</a>
    </li>
    {{/each}}

    
</ul>