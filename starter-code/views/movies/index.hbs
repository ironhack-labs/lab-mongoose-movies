<h2>Movies</h2>
<ul class="list-group list-group-flush">
  {{#each movies}}
  <li class="list-group-item dblist">
    <a href="/movies/{{this._id}}">{{this.title}}</a>
    <div>
      <form action="/movies/edit/{{this._id}}" method="GET">
      <button type="submit" class="edit">Edit</button>
      </form>
      <form action="/movies/delete/{{this._id}}" method="GET">
      <button type="submit" class="delete">Delete</button>
      </form>
    </div>
  </li>
  {{/each}}
</ul>
<br>
<button type="button" class="btn btn-primary"><a href="/movies/new" style="text-decoration: none; color:white">Add Movie</a></button>