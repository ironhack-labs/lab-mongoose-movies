<h2>Movies</h2>
<ul>
{{#each movies}}
<br>
<li><a href="/movies/{{this._id}}">{{this.title}}</a></li>
<br>
<form action='/movies/{{this._id}}/delete' method='POST'>
  <button type='submit'>DELETE</button>
</form>
<a href="/movies/edit?_id={{this._id}}" >
      EDIT
    </a>
{{/each}}
</ul>
<br>
<a href="/movies">RETURN</a>
<a href="/movies/new">CREATE NEW MOVIE</a>