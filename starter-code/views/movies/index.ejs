<h2>Movies</h2>
<ul>
    <%movies.forEach(movie => {%>
      <li>
      <h3><%=movie.title%></h3>
      <a href="/movies/<%=movie._id%>">View Details</a>
      <br>
      <br>
      <form  action="/movies/<%=movie._id%>/delete" method="post">
        <button>Delete</button>
      </form>
      </li>
    <%})%>
</ul>
