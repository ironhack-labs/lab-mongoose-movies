<h2>Movies</h2>
<ul>
  <% movies.forEach((movie) => { %>
    <li>
      Movie: <%= movie.name %> - Genre: <%= movie.genre %>
      <a href="/movies/<%= movie._id %>">View Details</a>
      <form action="/movies/<%= movie._id %>/delete" method="POST">
        <button type="submit">Delete</button>
      <a href="/movies/<%= movie._id%>/edit">Edit</a>
    </li>
  <%  }) %>
</ul>