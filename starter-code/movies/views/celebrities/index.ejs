<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Celebrity</title>
  </head>
  <body>
    <h2>Celebrities</h2>
    <!-- Lets call the celebrities and do a for each -->
    <%  celebrities.forEach(( celebrity) => { %>
    
      <p>Name: <%=  celebrity.name %> | Occupation: <%=  celebrity.occupation %> | Catch Phrase: <%= celebrity.catchPhrase %> </p>
    <% }) %>
  </body>
</html>
