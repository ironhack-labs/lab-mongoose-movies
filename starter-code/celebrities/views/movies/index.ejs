<h2>Movies</h2>

<ul>
    <% movies.forEach((movie) => { %>
        <li>
            <%= movie.title %>
                <a href="/movies/<%= movie._id %>">View Details</a>
                <form action="/movies/<%= movie._id %>/delete" method="POST">
                    <button type="submit">Delete </button>
                </form>
        </li>
        <%}); %>
</ul>