<h2>Celebrities</h2>

<%celebrities.forEach((e) => {%>
<%=e.name%> | <a href= "/celebrities/<%= e.id %>">View more Details</a><br>

<a href="/celebrities/new">Create a new Celebrity</a>

<form action = "/celebrities/<%= e.id %>/delete" method="POST">
  <button type="submit">Delete</button>
  <% }) %>
</form>
