<h2>Movies List</h2>

<ul>
  <% movies.forEach((movie) => { %>
    <li>
      <%= movie.title %>
      <a href="/movies/<%= movie._id %>">View Details</a>
      <form action="/movies/<%= movie._id %>/delete" method="POST">
        <button type="submit">Delete</button>
      </form>
    </li>
  <%  }) %>
</ul>
<a href="./movies/new">New Movie</a>
<a href="../">Go back</a>
