<h2>Movies</h2>

<ul>
	<% movies.forEach((movie) => { %>
	<li>
		<%= movie.title %>
	</li>
	
	<button><a href="/movies/<%= movie._id %>/show">Movie Details</a></button>
	<form action="/movies/<%= movie._id %>/delete" method="POST">
		<button type="submit">Delete</button>
	</form>
<% }) %>
</ul>

<button><a href="/movies/new">Add a New Movie</a></button>
<button><a href="/">Home</a></button>