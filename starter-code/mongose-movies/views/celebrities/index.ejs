<h2> Celebrities list</h2>
<ul>
  <% data.forEach((e)=>{%>
    <li>
      Name : <%=e.name%>
      <a href="/celebrities/<%=e._id%>">Details</a>
      <form action='/celebrities/<%=e._id%>/delete' method="post">
      <button type='submit'> Delete</button>
    </form>
  </li>
    <%});%>
</ul>
<a href="/celebrities/new">Add a new Celebrity</a>
