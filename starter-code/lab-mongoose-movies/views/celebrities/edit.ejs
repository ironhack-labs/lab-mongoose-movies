<h2>Edit celebrity</h2>
<form class="new-celeb" action="http://localhost:3000/celebrities/<%=data._id%>" method="post">
  <label for="name">Name</label>
  <input type="text" name="name" value="<%=data.name%>">
  <label for="occupation">Occupation</label>
  <input type="text" name="occupation" value="<%=data.occupation%>">
  <label for="catchPhrase">Catch phrase</label>
  <input type="text" name="catchPhrase" value="<%=data.catchPhrase%>">
  <button type="submit" name="button">Save</button>
</form>
